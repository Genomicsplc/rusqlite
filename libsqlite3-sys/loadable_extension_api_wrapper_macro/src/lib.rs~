extern crate proc_macro;
use proc_macro::TokenStream;

#[proc_macro]
pub fn loadable_extension_api_wrapper(item: TokenStream) -> TokenStream {
    println!("loadable_extension_api_wrapper: item = {:?}", item);
    "fn answer() -> u32 { 42 }".parse().unwrap()
}

#[cfg(test)]
mod tests {
    #[test]
    fn it_works() {
        assert_eq!(2 + 2, 4);
    }
}
